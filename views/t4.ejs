<body style="background-color: rgb(4, 78, 78);">
        <div > 
                <h1 style="text-align: center;background-color: lightblue;">PROJECTS</h1>
                <%finals.forEach(function(finals){%>
                    
                <div style="float: left;width: 100%;">
                        <img style="float: right;"src="prof.png" width="200px" height="200px">
                    <h3>ID ::<span><%= finals.id%></span></h3> 
                    </br>
                    <h3>PROJECT_NAME ::<span><%= finals.project_name%></span></h3> 
                    </br>
                    <h3>FUND_PROVIDED ::<span><%= finals.funds_provided%></span></h3> 
                    </br>
                    <h3> FUND_SPENT ::<span><%= finals.funs_spent%></span></h3> 
                    </br>
                    <h3> YEAR ::<span><%= finals.year%></span></h3> 
                    </br>
                    <h3> STATUS ::<span><%= finals.status%></span></h3>
                    </br>
                 <hr>
                </div>
                <%});%>
        </div>
        </body>